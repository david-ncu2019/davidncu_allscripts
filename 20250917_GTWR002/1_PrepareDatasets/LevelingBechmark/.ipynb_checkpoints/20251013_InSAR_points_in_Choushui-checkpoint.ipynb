   "id": "ee4de178-79b6-4a0d-ae49-206c07ba3bea",
   "metadata": {},
   "source": [
    "#### search for InSAR points located within Choushui shapefile"
   ]
  },
  {
   "cell_type": "raw",
   "id": "59c10c8d-b763-4c3b-977e-0490146f2173",
   "metadata": {},
   "source": [
    "fpath = r\"D:\\1000_SCRIPTS\\003_Project002\\20250917_GTWR002\\2_KrigingInterpolation\\5_PostKriging\\Monthly_CUMDISP_saveqgis_Oct2025.xz\"\n",
    "df = pd.read_pickle(fpath)\n",
    "point_df = df.loc[:, [\"PointKey\", \"X_TWD97\", \"Y_TWD97\"]]\n",
    "point_df = geospatial.convert_to_geodata(\n",
    "    df=point_df,\n",
    "    xcoord_col=\"X_TWD97\",\n",
    "    ycoord_col=\"Y_TWD97\",\n",
    "    crs_epsg=\"EPSG:3826\",\n",
    ")\n",
    "point_df.head(5)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "d5d9a6e8-2270-4581-b972-c7d0974fddaa",
   "metadata": {},
   "source": [
    "buffer_gdf = gpd.read_file(\n",
    "    r\"E:\\002_ARCGIS_WORK\\2021邊界條件討論\\choushui_modified_2021_TWD97.shp\"\n",
    ")\n",
    "points_within_polygon = geospatial.find_points_within_polygon(\n",
    "    points_gdf=point_df, polygon_geom=buffer_gdf\n",
    ")\n",
    "show(points_within_polygon)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "eee86d5e-0f68-4ed5-8b8f-a4a894736717",
   "metadata": {},
   "source": [
    "points_within_polygon_final = points_within_polygon.loc[\n",
    "    :, point_df.columns[:-1]\n",
    "]\n",
    "# points_within_polygon_final\n",
    "# points_within_polygon.to_pickle(\"CUMDISP_within_CRFP_Area_saveqgis_Oct2025.xz\")"
   ]
  },
  {
   "cell_type": "raw",
   "id": "802086f3-18c1-4ecf-86fe-d18f2e5da071",
   "metadata": {},
   "source": [
    "# load timeseries of cumulative displacement of all points\n",
    "cumdisp_df = pd.read_pickle(\n",
    "    r\"D:\\1000_SCRIPTS\\003_Project002\\20250917_GTWR002\\2_KrigingInterpolation\\5_PostKriging\\Monthly_CUMDISP_saveqgis_Oct2025.xz\"\n",
    ")\n",
    "cumdisp_df = cumdisp_df.set_index(\"PointKey\")\n",
    "show(cumdisp_df)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "67877588-8fb5-49cc-aa6a-db77aec97664",
   "metadata": {},
   "source": [
    "cumdisp_df.loc[points_within_polygon[\"PointKey\"].tolist(), :].to_pickle(\n",
    "    r\"Monthly_CUMDISP_in_CRFP_saveqgis_Oct2025.xz\"\n",
    ")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "c78e6222-6472-4607-b89f-42e511a311ac",
   "metadata": {},
   "source": [
    "#### InSAR points located within leveling buffer"
   ]
  },
  {
   "cell_type": "raw",
   "id": "ae1c356c-8084-4bda-873b-d47d29b4db4e",
   "metadata": {},
   "source": [
    "fpath = r\"D:\\1000_SCRIPTS\\003_Project002\\20250917_GTWR002\\2_KrigingInterpolation\\5_PostKriging\\Monthly_CUMDISP_saveqgis_Oct2025.xz\"\n",
    "df = pd.read_pickle(fpath)\n",
    "point_df = df.loc[:, [\"PointKey\", \"X_TWD97\", \"Y_TWD97\"]]\n",
    "point_df = geospatial.convert_to_geodata(\n",
    "    df=point_df,\n",
    "    xcoord_col=\"X_TWD97\",\n",
    "    ycoord_col=\"Y_TWD97\",\n",
    "    crs_epsg=\"EPSG:3826\",\n",
    ")\n",
    "point_df.head(5)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "30a493e8-da1c-487d-aa55-a92055afd188",
   "metadata": {},
   "source": [
    "changhua_leveling_gdf = pd.read_pickle(\n",
    "    r\"Changhua_Leveling_2024_modified2016_2024.xz\"\n",
    ")\n",
    "changhua_leveling_gdf = geospatial.convert_to_geodata(\n",
    "    df=changhua_leveling_gdf,\n",
    "    xcoord_col=\"X_TWD97\",\n",
    "    ycoord_col=\"Y_TWD97\",\n",
    "    crs_epsg=\"EPSG:3826\",\n",
    ")\n",
    "changhua_leveling_gdf = changhua_leveling_gdf.reset_index(drop=False)\n",
    "# - - - - - - - - - - - - - - - - - - - - - - - - - - - -\n",
    "yunlin_leveling_gdf = pd.read_pickle(\n",
    "    r\"Yunlin_Leveling_2024_modified2016_2024.xz\"\n",
    ")\n",
    "\n",
    "yunlin_leveling_gdf = geospatial.convert_to_geodata(\n",
    "    df=yunlin_leveling_gdf,\n",
    "    xcoord_col=\"X_TWD97\",\n",
    "    ycoord_col=\"Y_TWD97\",\n",
    "    crs_epsg=\"EPSG:3826\",\n",
    ")\n",
    "\n",
    "yunlin_leveling_gdf = yunlin_leveling_gdf.reset_index(drop=False)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "2686eba2-78d4-4f2e-bfff-6cd9a47f0290",
   "metadata": {},
   "source": [
    "insarpoints_in_buffer = yunlin_leveling_gdf.apply(\n",
    "    lambda row: find_point_neighbors(row, point_df, \"ALTCODE\", 200), axis=1\n",
    ")\n",
    "\n",
    "result_gdf = pd.concat(insarpoints_in_buffer.tolist(), ignore_index=True)\n",
    "result_gdf.loc[:, result_gdf.columns[:-1]].to_pickle(\n",
    "    \"CUMDISP_in_YunlinLeveling_200m.xz\"\n",
    ")"
   ]
  },
