   "source": [
    "main_df = pd.read_pickle(r\"yunlin_sbasinsar/monthly_vert_yunlin.pkl\", compression=\"zip\")\n",
    "main_df.head(5)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "ce3e9f06-a404-4d70-a3b9-8884fa30cd6b",
   "metadata": {},
   "source": [
    "#### Calculate annual average velocity"
   ]
  },
  {
   "cell_type": "raw",
   "id": "3284871a-7e54-4023-9837-100f8bd70a40",
   "metadata": {},
   "source": [
    "main_df = main_df.iloc[:100, :]\n",
    "measure_cols = [col for col in main_df.columns if col.startswith(\"D\")]\n",
    "\n",
    "yearly_anchor_dates = [f\"D{year}0501\" for year in range(2016, 2025)]\n",
    "\n",
    "final_result = pd.DataFrame(data=None, index=main_df.index)\n",
    "\n",
    "for idx in range(2):\n",
    "    # idx = 0\n",
    "    current_year = yearly_anchor_dates[idx]\n",
    "    next_year = yearly_anchor_dates[idx + 1]\n",
    "\n",
    "    print(f\"Current period {current_year[1:]} - {next_year[1:]}\")\n",
    "\n",
    "    cumdisp_byYear = main_df.loc[:, current_year:next_year]\n",
    "    cumdisp_byYear.columns = pd.to_datetime([col[1:] for col in cumdisp_byYear.columns])\n",
    "\n",
    "    velocities = cumdisp_byYear.apply(analysis.get_linear_trend, axis=1)\n",
    "\n",
    "    velocity_df = pd.DataFrame(\n",
    "        [velocity[1] for velocity in velocities],\n",
    "        columns=[\"Velocity_mm_yr\"],\n",
    "        index=cumdisp_byYear.index,\n",
    "    )\n",
    "    velocity_df.iloc[:, 0] = velocity_df.iloc[:, 0] * 365.25\n",
    "    final_result[f\"{current_year}_{next_year}\"] = final_result.index.map(\n",
    "        velocity_df[\"Velocity_mm_yr\"]\n",
    "    )\n",
    "\n",
    "final_result"
   ]
  }
