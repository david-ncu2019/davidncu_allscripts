    "df = pd.DataFrame(\n",
    "    {station: mlcw_info[station] for station in available_stations}\n",
    ").T\n",
    "columns2drop = [\n",
    "    \"classify\",\n",
    "    \"monthly_values\",\n",
    "    \"values\",\n",
    "    \"Update001\",\n",
    "    \"SetTime\",\n",
    "    \"CountyName\",\n",
    "    \"GroundWaterZoneName\",\n",
    "    \"LandSubsidenceMonitoringWellName\",\n",
    "    \"TownName\",\n",
    "    \"檔案名稱\",\n",
    "    \"表格對照\",\n",
    "]\n",
    "df = df.drop(labels=columns2drop, axis=1).reset_index()\n",
    "df = df.rename(\n",
    "    {\n",
    "        \"index\": \"STATION\",\n",
    "        \"LocationByWGS84_Latitude\": \"Y_WGS84\",\n",
    "        \"LocationByWGS84_Longitude\": \"X_WGS84\",\n",
    "    },\n",
    "    axis=1,\n",
    ")\n",
    "gdf = geospatial.convert_to_geodata(df=df, xcoord_col=\"X_WGS84\", ycoord_col=\"Y_WGS84\", crs_epsg=\"EPSG:4326\")\n",
    "gdf.to_file(\"points_fld/mlcw_wgs84.shp\")"
